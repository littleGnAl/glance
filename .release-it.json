{
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "ignoreRecommendedBump": "true",
      "header": "# CHANGELOG",
      "preset": {
        "name": "conventionalcommits",
        "types": [
          {
            "type": "feat",
            "section": "Features"
          },
          {
            "type": "fix",
            "section": "Bug Fixes"
          },
          {
            "type": "chore"
          }
        ]
      }
    },
    "@release-it/bumper": {
      "in": "pubspec.yaml",
      "out": "pubspec.yaml"
    }
  }
}